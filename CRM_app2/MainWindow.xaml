<Window x:Class="CRM_app2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CRM_app2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Клиентский учет" FontSize="30" HorizontalAlignment="Center" FontFamily="Cascadia Code Light"></TextBlock>
        <Grid Background="#DBD9DF" Panel.ZIndex="-2"/>
        <Grid Grid.Row ="2" Background="#B1ABBE"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame"></Frame>
        <Button Content="Назад" Name="BtnBack" HorizontalAlignment="right" Click="BtnBack_Click"/>
    </Grid>
</Window>
